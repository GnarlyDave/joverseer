<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.joverseer.ui.viewers.HexInfoController">
   <children>
      <HBox>
         <children>
            <HBox>
               <children>
                  <Label id="lblHexNo" text="%HexInfoViewer.HexNumColon" />
                  <Label fx:id="valHexNo" text="nnnn" />
                  <HBox>
                     <children>
                        <Label fx:id="valTerrain" text="Label" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="%HexInfoViewer.LatestInfoColon" />
            <Label fx:id="valTurnInfo"/>
         </children></HBox>
      <Separator prefWidth="200.0" />
      <TableView fx:id="iMovementCost">
        <columns>
            <TableColumn fx:id="iDirection" prefWidth="75.0" />
          <TableColumn fx:id="iInfantryCost" editable="false" prefWidth="75.0" sortable="false" text="%HexInfoViewer.AbbInfantry" />
          <TableColumn fx:id="iCavalryCost" editable="false" prefWidth="75.0" sortable="false" text="%HexInfoViewer.AbbCavalry" />
        </columns>
      </TableView>
   </children>
</VBox>
